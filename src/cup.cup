              ;

arrayInit ::= expression:e {:
                NodoArbol result = new NodoArbol("arrayInit");
                result.agregarHijo((NodoArbol)e);
                RESULT = result;
              :}
            | arrayInit:ai COMMA expression:e {:
                NodoArbol result = new NodoArbol("arrayInit");
                result.agregarHijo((NodoArbol)ai);
                result.agregarHijo(new NodoArbol("COMMA"));
                result.agregarHijo((NodoArbol)e);
                RESULT = result;
              :}
            ;

decide ::= DECIDE OF condicionesDecide:cd ELSE bloque:b END DECIDE ENDL {:
                NodoArbol result = new NodoArbol("decide_with_else");
                result.agregarHijo(new NodoArbol("DECIDE"));
                result.agregarHijo(new NodoArbol("OF"));
                result.agregarHijo((NodoArbol)cd);
                result.agregarHijo(new NodoArbol("ELSE"));
                result.agregarHijo((NodoArbol)b);
                result.agregarHijo(new NodoArbol("END"));
                result.agregarHijo(new NodoArbol("DECIDE"));
                result.agregarHijo(new NodoArbol("ENDL"));
                RESULT = result;
          :}
         | DECIDE OF condicionesDecide:cd END DECIDE ENDL {:
                NodoArbol result = new NodoArbol("decide");
                result.agregarHijo(new NodoArbol("DECIDE"));
                result.agregarHijo(new NodoArbol("OF"));
                result.agregarHijo((NodoArbol)cd);
                result.agregarHijo(new NodoArbol("END"));
                result.agregarHijo(new NodoArbol("DECIDE"));
                result.agregarHijo(new NodoArbol("ENDL"));
                RESULT = result;
          :}
         ;

condicionesDecide ::= condicionDecide:cd {:
                NodoArbol result = new NodoArbol("condicionesDecide");
                result.agregarHijo((NodoArbol)cd);
                RESULT = result;
              :}
                    | condicionesDecide:cds condicionDecide:cd {:
                NodoArbol result = new NodoArbol("condicionesDecide");
                result.agregarHijo((NodoArbol)cds);
                result.agregarHijo((NodoArbol)cd);
                RESULT = result;
              :}
                    ;

condicionDecide ::= expression:e bloque:b {:
                NodoArbol result = new NodoArbol("condicionDecide");
                result.agregarHijo((NodoArbol)e);
                result.agregarHijo((NodoArbol)b);
                RESULT = result;
              :};

loop ::= LOOP listaInstr:li EXIT WHEN expression:e ENDL END LOOP ENDL {:
                NodoArbol result = new NodoArbol("loop");
                result.agregarHijo(new NodoArbol("LOOP"));
                result.agregarHijo((NodoArbol)li);
                result.agregarHijo(new NodoArbol("EXIT"));
                result.agregarHijo(new NodoArbol("WHEN"));
                result.agregarHijo((NodoArbol)e);
                result.agregarHijo(new NodoArbol("ENDL"));
                result.agregarHijo(new NodoArbol("END"));
                result.agregarHijo(new NodoArbol("LOOP"));
                result.agregarHijo(new NodoArbol("ENDL"));
                RESULT = result;
          :};

for_stmt ::= FOR LPAREN expression:e1 SEMI expression:e2 SEMI expression:e3 RPAREN bloque:b ENDL {:
                NodoArbol result = new NodoArbol("for_stmt");
                result.agregarHijo(new NodoArbol("FOR"));
                result.agregarHijo(new NodoArbol("LPAREN"));
                result.agregarHijo((NodoArbol)e1);
                result.agregarHijo(new NodoArbol("SEMI"));
                result.agregarHijo((NodoArbol)e2);
                result.agregarHijo(new NodoArbol("SEMI"));
                result.agregarHijo((NodoArbol)e3);
                result.agregarHijo(new NodoArbol("RPAREN"));
                result.agregarHijo((NodoArbol)b);
                result.agregarHijo(new NodoArbol("ENDL"));
                RESULT = result;
          :};

/* Expresiones (ya proporcionadas anteriormente) */
expression ::= expression:e1 PLUS expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("+");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 MINUS expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("-");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 GE expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol(">=");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 EQ expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("==");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 TIMES expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("*");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 DIV_INT expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("//");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 POW expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("^");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 LT expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("<");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 DIV_FLOAT expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("/");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 LE expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("<=");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 MOD expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("%");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 GT expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol(">");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 NE expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("!=");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 AND expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("@");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| expression:e1 OR expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("~");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| NOT expression:e1 {:
                            NodoArbol hijoExpr = (NodoArbol)e1;
                            NodoArbol result = new NodoArbol("Σ");
                            result.agregarHijo(hijoExpr);
                            RESULT = result;
:}
| MINUS expression:e1 {:
                            NodoArbol hijoExpr = (NodoArbol)e1;
                            NodoArbol result = new NodoArbol("MINUS");
                            result.agregarHijo(hijoExpr);
                            RESULT = result;
:}
| INC expression:e1 {:
                            NodoArbol hijoExpr = (NodoArbol)e1;
                            NodoArbol result = new NodoArbol("++");
                            result.agregarHijo(hijoExpr);
                            RESULT = result;
:}
| DEC expression:e1 {:
                            NodoArbol hijoExpr = (NodoArbol)e1;
                            NodoArbol result = new NodoArbol("--");
                            result.agregarHijo(hijoExpr);
                            RESULT = result;
:}
| expression:e1 ASSIGN expression:e2 {:
                            NodoArbol hijoIzq = (NodoArbol)e1;
                            NodoArbol hijoDer = (NodoArbol)e2;
                            NodoArbol result = new NodoArbol("=");
                            result.agregarHijo(hijoIzq);
                            result.agregarHijo(hijoDer);
                            RESULT = result;
:}
| LPAREN expression:e1 RPAREN {:
                            NodoArbol hijoExpr = (NodoArbol)e1;
                            NodoArbol result = new NodoArbol("()");
                            result.agregarHijo(new NodoArbol("¿"));
                            result.agregarHijo(hijoExpr);
                            result.agregarHijo(new NodoArbol("?"));
                            RESULT = result;
:}
| INTEGER_LITERAL:e1 {:
                            NodoArbol result = new NodoArbol("int_literal", e1.toString());
                            RESULT = result;
:}
| FLOAT_LITERAL:e1 {:
                            NodoArbol result = new NodoArbol("float_literal", e1.toString());
                            RESULT = result;
:}
| BOOLEAN_LITERAL:e1 {:
                            NodoArbol result = new NodoArbol("bool_literal", e1.toString());
                            RESULT = result;
:}
| CHAR_LITERAL:e1 {:
                            NodoArbol result = new NodoArbol("char_literal", e1.toString());
                            RESULT = result;
:}
| STRING_LITERAL:e1 {:
                            NodoArbol result = new NodoArbol("string_literal", e1.toString());
                            RESULT = result;
:}
| usoArray
| llamadaFuncion
;

usoArray ::= IDENTIFIER:id LBRACKET expression:expr1 RBRACKET LBRACKET expression:expr2 RBRACKET {:
                            NodoArbol result = new NodoArbol("array_access");
                            result.agregarHijo(new NodoArbol("IDENTIFIER", id.toString()));
                            result.agregarHijo(new NodoArbol("¡"));
                            result.agregarHijo((NodoArbol)expr1);
                            result.agregarHijo(new NodoArbol("!"));
                            result.agregarHijo(new NodoArbol("¡"));
                            result.agregarHijo((NodoArbol)expr2);
                            result.agregarHijo(new NodoArbol("!"));
                            RESULT = result;
:};

llamadaFuncion ::= IDENTIFIER:id LPAREN listaArgumentos:args RPAREN {:
                            NodoArbol result = new NodoArbol("function_call");
                            result.agregarHijo(new NodoArbol("IDENTIFIER", id.toString()));
                            result.agregarHijo(new NodoArbol("¿"));
                            if (args != null) {
                                result.agregarHijo((NodoArbol)args);
                            }
                            result.agregarHijo(new NodoArbol("?"));
                            RESULT = result;
:};

listaArgumentos ::= 
            {: /* vacío */ RESULT = null; :}
        | expression:expr {:
                            NodoArbol result = new NodoArbol("listaArgumentos");
                            result.agregarHijo((NodoArbol)expr);
                            RESULT = result;
:}
        | listaArgumentos:args COMMA expression:expr {:
                            NodoArbol result = new NodoArbol("listaArgumentos");
                            result.agregarHijo((NodoArbol)args);
                            result.agregarHijo(new NodoArbol("COMMA"));
                            result.agregarHijo((NodoArbol)expr);
                            RESULT = result;
:};